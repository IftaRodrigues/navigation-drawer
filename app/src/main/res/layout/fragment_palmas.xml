<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:layout_margin="10dp"
    tools:context=".ui.palmas.PalmasFagment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="159dp"
            android:src="@drawable/palmas"
            android:layout_gravity="center"
            />

        <TextView
            android:id="@+id/text_cursos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/cursos_ofertados"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:layout_gravity="center"/>

        <TextView
            android:id="@+id/text_fic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/fic"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:text="@string/fic_qualificacao"/>

        <TextView
            android:id="@+id/text_proeja"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="viewEnd"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
            android:text="@string/proeja"/>

        <TextView
            android:id="@+id/text_atendimento"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/atendimento"/>

        <TextView
            android:id="@+id/text_manutencao_e_operacao_de_microcomputadores"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/manutencao_e_operacao_de_microcomputadores"/>

        <TextView
            android:id="@+id/text_tecnicos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/tecnicos"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:text="@string/tecnicos"/>

        <TextView
            android:id="@+id/text_integrado"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="viewEnd"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
            android:text="@string/integrado_regular"/>

        <TextView
            android:id="@+id/text_tecnico_em_administracao"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_administracao"/>
        <TextView
            android:id="@+id/text_tecnico_em_agrimensura"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_agrimensura"/>
        <TextView
            android:id="@+id/text_tecnico_em_agronegocio"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_agronegocio"/>
        <TextView
            android:id="@+id/text_tecnico_em_controle_ambiental"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_controle_ambiental"/>
        <TextView
            android:id="@+id/text_tecnico_em_eletrotocnica"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_eletrotocnica"/>
        <TextView
            android:id="@+id/text_tecnico_em_eventos"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_eventos"/>
        <TextView
            android:id="@+id/text_tecnico_em_informatica_para_internet"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_informatica_para_internet"/>
        <TextView
            android:id="@+id/text_tecnico_em_mecatronica"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_mecatronica"/>

        <TextView
            android:id="@+id/text_subsequente"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="viewEnd"
            android:layout_marginTop="10dp"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
            android:text="@string/subsequente"/>

        <TextView
            android:id="@+id/text_tecnico_em_agrimensura_subsequente"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_agrimensura"/>
        <TextView
            android:id="@+id/text_tecnico_em_automacao_industrial"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_automacao_industrial"/>
        <TextView
            android:id="@+id/tecnico_em_edificacoes"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_edificacoes"/>
        <TextView
            android:id="@+id/text_tecnico_em_eletrotocnica_susequente"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_eletrotocnica"/>
        <TextView
            android:id="@+id/text_tecnico_em_informatica"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_informatica"/>
        <TextView
            android:id="@+id/text_tecnico_em_secretariado"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_secretariado"/>
        <TextView
            android:id="@+id/text_tecnico_em_seguranca_do_trabalho"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tecnico_em_seguranca_do_trabalho"/>

        <TextView
            android:id="@+id/text_graduacao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/graducao"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:text="@string/graduacao"/>

        <TextView
            android:id="@+id/text_bacharelado"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="viewEnd"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
            android:text="@string/bacharelado"/>

