            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/gestao_publica"/>
        <TextView
            android:id="@+id/text_sistemas_eletricos"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/sistemas_eletricos"/>

        <TextView
            android:id="@+id/text_sistemas_para_internet"
            android:textAlignment="viewEnd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/sistemas_para_internet"/>

        <TextView
            android:id="@+id/text_pos_graduacao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/pos_graduacao"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:text="@string/pos_graduacao"/>

        <TextView
            android:id="@+id/text_especializacao_lato_sensu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="viewEnd"
